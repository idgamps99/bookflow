<%= @book.title %>
<%= @book.author %>

<% if params[:query].present? %>
<ul>
  <% @books.each do |book|  %>
    <li>Title: <%= book["title"] %></li>
    <li>Author: <%= book["authors"] %> </li>
    <li>Year Published <%= book["publishedDate"] %> </li>
    <li>Description: <%= book["description"] %> </li>
  <% end %>
</ul>
<% end %>
